{
    "service" : {
            "ip" 	: "192.168.1.5",
            "api" 	: "http",
            "port" 	: 8080
        },
    "server" :
        {
            "mod" 	: "thread"
        },
    "http" : 
        {
            "script_names" : ["/"]
        },
    "cache" :
        {
            "backend" 	: "thread_shared",
            "limit"	: 100000
        },
    "session" : 
        {  
            "expire" 	: "renew",  
            "timeout" 	: 604800,  
            "location" 	: "server",  
            "server" 	:      
                {  
                    "storage" 	:        "files",  
                    "dir"   	:   "./.session"
                }      
        },
    "security" 		:	 
        {
            "csrf" 	: 
            {
                "enable" 	: "true"
            }
        }
}
